{
  "name": "todo-angular-redux",
  "version": "0.3.0",
  "description": "Todo app with Angular 1.5 and Redux",
  "homepage": "https://github.com/r-park/todo-angular-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/todo-angular-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=5.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --display-modules --progress",
    "clean": "del-cli target",
    "create:db": "node db.js",
    "lint": "eslint -c .eslintrc.yml .",
    "postinstall": "npm run create:db",
    "prebuild": "npm-run-all clean lint test",
    "server:api": "cross-env NODE_ENV=development json-server db.json -p 3001 -w",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:api & npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start"
  },
  "devDependencies": {
    "angular": "~1.5.6",
    "angular-mocks": "~1.5.6",
    "angular-ui-router": "~0.3.0",
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.9.1",
    "babel-eslint": "~6.0.4",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-polyfill": "~6.9.1",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-stage-1": "~6.5.0",
    "cross-env": "~1.0.8",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "~2.11.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.19.0",
    "jasmine-core": "~2.4.1",
    "json-server": "~0.8.14",
    "karma": "~0.13.22",
    "karma-chrome-launcher": "~1.0.1",
    "karma-jasmine": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "lodash": "~4.13.1",
    "minx": "r-park/minx.git",
    "ng-redux": "3.3.4-beta.0",
    "node-sass": "~3.7.0",
    "npm-run-all": "~2.1.1",
    "postcss-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
    "redux": "~3.5.2",
    "redux-logger": "~2.6.1",
    "sass-loader": "~3.2.0",
    "sinon": "~1.17.4",
    "style-loader": "~0.13.1",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1"
  }
}
